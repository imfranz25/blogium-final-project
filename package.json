{
  "name": "blogium-parse-server",
  "version": "1.4.0",
  "description": "Blogium - Parse Server Version",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParsePlatform/parse-server-example"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc jasmine",
    "lint": "eslint --cache ./cloud && eslint --cache index.js && eslint --cache ./spec",
    "lint-fix": "eslint --cache --fix ./cloud && eslint --cache --fix index.js && eslint --cache --fix ./spec",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "format": "yarn prettier --write",
    "dev": "concurrently --kill-others \"npm run server\"",
    "server": "cross-env NODE_ENV=development nodemon ./index.js",
    "start": "cross-env NODE_ENV=production node ./index.js",
    "test": "mongodb-runner start && jasmine",
    "watch": "nodemon index.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.18.1",
    "parse": "3.4.2",
    "parse-dashboard": "^5.0.0",
    "parse-server": "5.2.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.15.0",
    "jasmine": "4.1.0",
    "mongodb-runner": "4.9.0",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2"
  },
  "engines": {
    "node": ">=12.22.10 <19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
